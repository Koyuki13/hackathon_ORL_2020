{% extends 'layout.html.twig' %}

{% block title %} HACKATHON AVRIL 2020{% endblock %}

{% block content %}
    <div class="container my-5">
        <img src="https://fontmeme.com/permalink/200429/1afad37328cfeeb3726dae82656354d9.png"
             alt="police-retour-vers-le-futur" border="0">

        <h2>La route ? Là où on va on a pas besoin de route !</h2>
        <div class="container-fluid">
            <div class="row">
                <div class="col-3">
                    <div class="row">
                        <h3>Epoque</h3>
                        <p>{{ period }}</p>
                    </div>
                </div>
                <div class="col-3">
                    <div class="row">
                        <h3>Année</h3>
                        <p>{{ museum.objectBeginDate }}<p>
                    </div>
                </div>
            </div>
        </div>

        <div class="tableMuseum row">
            <div class="imageShow col">
                <img class="shadow" src="{{ museum.primaryImageSmall }}" alt="{{ museum.title }}">
            </div>
        </div>
    </div>
{% endblock %}
